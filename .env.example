# ==========================================
# Kerberos POC Environment Configuration
# ==========================================
# Copy this file to .env and update with your values:
# cp .env.example .env

# ===================
# Proxy Configuration
# ===================
PROXY_HOST=
PROXY_PORT=

# =======================
# Kerberos Configuration
# =======================
KERBEROS_REALM=
KERBEROS_PRINCIPAL=

# ===================
# File Paths
# ===================
# Absolute paths or relative to project root
KEYTAB_FILE=
KRB5_CONF=

# ==================
# Testing Configuration
# ==================
TEST_URL=https://www.google.com
APPLICATION_NAME=CUBE

# ==================
# Optional Settings
# ==================
# Set to true/false or 1/0
DRY_RUN=false
NO_TEST=false
DEBUG=false

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
